<%= form_for @changeset, id="new-message", @action, fn f -> %>
  <%= if @changeset.action do %>
    <div class="alert alert-danger">
      <p>Oops, something went wrong! Please check the errors below.</p>
    </div>
  <% end %>

  <%= label f, "Введите ИНН" %>
  <div>
    <%= text_input f, :number %>
    <%= error_tag f, :number %>
    <%= submit "Отправить" %>
  </div>
<% end %>
